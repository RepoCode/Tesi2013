<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tesi.bticino.scenarioOpenWebNet"
    android:versionCode="3"
    android:versionName="3.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <uses-configuration android:reqTouchScreen="finger" />

    <uses-feature android:glEsVersion="0x00020000" />

    <supports-screens android:anyDensity="true" />

    <application
        android:allowBackup="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.Bticinotheme" >
        <activity
            android:name="tesi.bticino.scenariodue.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name="tesi.bticino.scenariodue.ServiceReceiver" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>

        <service android:name="tesi.bticino.scenariodue.IncomingService" />

        <activity
            android:name="tesi.bticino.scenariodue.VideoSurveillance"
            android:label="@string/title_activity_video_surveillance"
            android:parentActivityName="tesi.bticino.scenariodue.MainActivity"
            android:screenOrientation="portrait" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="tesi.bticino.scenariodue.MainActivity" />
        </activity>
        <activity
            android:name="tesi.bticino.scenariodue.TestAudio"
            android:label="@string/title_activity_audio"
            android:parentActivityName="tesi.bticino.scenariodue.MainActivity"
            android:screenOrientation="portrait" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="tesi.bticino.scenariodue.MainActivity" />
        </activity>
        <activity
            android:name="tesi.bticino.scenariodue.IncomingCall"
            android:label="@string/title_activity_incoming_call"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="tesi.bticino.scenariodue.Information"
            android:label="@string/title_activity_information"
            android:parentActivityName="tesi.bticino.scenariodue.MainActivity"
            android:screenOrientation="portrait" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="tesi.bticino.scenariodue.MainActivity" />
        </activity>
        <activity
            android:name="tesi.bticino.scenariodue.MainActivity33"
            android:label="@string/title_activity_main_activity33"
            android:parentActivityName="tesi.bticino.scenariodue.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="tesi.bticino.scenariodue.MainActivity" />
        </activity>
    </application>

</manifest>
